{
	"name": "pcm",
	"displayName": "Zeiss PCM",
	"description": "Zeiss PCM language syntax highlighting support for VSCode.",
	"version": "1.0.0",
	"icon": "images/logo.png",
	"publisher": "A9G-Data-Droid",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/A9G-Data-Droid/vscode-pcm.git"
	},
	"activationEvents": [
		"onLanguage:pcm"
	],
	"contributes": {
		"languages": [
			{
				"id": "pcm",
				"aliases": [
					"PCM",
					"pcm"
				],
				"extensions": [
					".pcm"
				],
				"configuration": "./pcm.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "pcm",
				"scopeName": "source.pcm",
				"path": "./syntaxes/pcm.json"
			}
		]
	}
}